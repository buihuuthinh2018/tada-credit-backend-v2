# Database
DATABASE_URL="mysql://root:[PASSWORD]@localhost:3306/[DATABASE_NAME]"

# JWT
JWT_SECRET="jwt_secret_key"
JWT_EXPIRES_SECONDS="3600s"
JWT_REFRESH_SECRET="jwt_refresh_secret_key"
JWT_REFRESH_EXPIRES_SECONDS="604800s"

# Telegram Storage (files are stored in a Telegram channel via Bot API)
TELEGRAM_BOT_TOKEN = "your_telegram_bot_token"
